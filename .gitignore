gradle-wrapper.jar
gradle-wrapper.properties

# OS generated files
.DS_Store
Thumbs.db

# Kotlin/Native output (if applicable)
.demo/.kcache
.demo/*.klib

# Project-specific Kotlin build cache
.demo/.kbuild/

